[global]
ioengine=libaio
direct=1
time_based
runtime=60
group_reporting
filename=/dev/nvme0n1p2
bs=4k
iodepth=16
numjobs=1
stonewall
percentile_list=95:99

[randread]
rw=randread

[randwrite]
rw=randwrite

[rand7030]
rw=randrw
rwmixread=70

[rand3070]
rw=randrw
rwmixread=30

[rand5050]
rw=randrw
rwmixread=50
